{
  "name": "agarra-la-pala",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:desktop": "npm-run-all --parallel  client desktop",
    "dev:server": "npm-run-all --parallel server client",
    "build:desktop": "vite build --config electron.vite.config.js",
    "build:server": "vite build --config server.vite.config.js",
    "build": "npm-run-all --parallel build:desktop build:server",
    "desktop": "electron electron/index.js",
    "server": "node --watch server/index.js",
    "client": "vite",
    "preview": "vite preview",
    "deploy": "cd server  && fly deploy"
  },
  "dependencies": {
    "@ai-sdk/google": "^0.0.27",
    "@electron/packager": "^18.3.3",
    "ai": "^3.2.32",
    "cors": "^2.8.5",
    "electron": "^31.2.1",
    "express": "^4.19.2",
    "marked": "^13.0.2",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "preact": "^10.22.1",
    "react-router-dom": "^6.25.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.3",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.6",
    "vite": "^5.3.4"
  }
}
